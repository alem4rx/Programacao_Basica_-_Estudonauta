//Exercicio 3
//  Faça um programa que peça para o usuário digitar um numero e mostre o seu antecessor e seu sucessor
programa
{
    
  funcao inicio()
  {
    inteiro	n
    escreva("\n Digite um numero: ")
    leia(n)
    escreva("\nSeu antecessor é ", n - 1)
    escreva("\nSeu sucessor é ", n + 1)
    escreva("\n\n")
  }
}